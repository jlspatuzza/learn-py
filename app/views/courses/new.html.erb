


    <%= form_with model: @course do |f| %>

       <%= f.label :name %>
    <%= f.text_field :name %>


    <%= f.label :description %>
    <%= f.text_field :description %>
    <%= f.label :date %>
    <%= f.date_field :date %>
    <%= f.label :start_time %>
    <%= f.time_field :start_time %>
    <%= f.label :end_time %>
    <%= f.time_field :end_time  %>
    <%= f.label :quantity %>
    <%= f.number_field :quantity, in: 1..1 %>
    <%= f.label :price%>
    <%= f.number_field :price, size: "40x5"  %>
    <%= f.label :num_seats %>
    <%= f.number_field :num_seats %>



    <%= f.submit %>
    <% end %>
